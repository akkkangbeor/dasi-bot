{
  "name": "discord-bot-boilerplate",
  "version": "1.0.0",
  "description": "Discord Bot Boilerplate",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "deploy-commands": "ts-node src/deploy-commands.ts"
  },
  "dependencies": {
    "@discordjs/builders": "^1.7.0",
    "@types/luxon": "^3.6.2",
    "axios": "^1.8.4",
    "cheerio": "^1.0.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "luxon": "^3.6.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^20.11.19",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
